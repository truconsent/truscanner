        {
            "name": "Job Title",
            "category": "Employment & Professional Data",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)\\bjob[^\\s]{0,5}(title|designation|position)\\b",
                "(?i)past[^\\s/(;)#|,=!>]{0,5}role|(?:previous|past)[^\\s/(;)#|,=!>]{0,5}job[^\\s/(;)#|,=!>]{0,5}title"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Role",
            "category": "Employment & Professional Data",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
