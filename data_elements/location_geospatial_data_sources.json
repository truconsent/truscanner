        },
        {
            "name": "Precise Location",
            "category": "Location & Geospatial Data",
            "isSensitive": false,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\b(gps[^\\s/(;)#|,=!>]{0,3}(?:location|position|trace)|user[^\\s/(;)#|,=!>]{0,5}location|latitude|longitude|geo[^\\s/(;)#|,=!>]{0,5}coordinates)\\b",
                "(?i)\\b(lat|lon|latlng|latlon)\\b(?![^\\s/(;)#|,=!>]*?(?:platform|standalone|clone|float|long|distance|translation|relative|calculate|update))"
            ],
            "tags": {
                "law": "GDPR"
            }
        }
    ]
