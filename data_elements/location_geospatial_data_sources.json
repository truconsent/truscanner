{
    "sources": [
        {
            "name": "Live GPS Location",
            "category": "Location & Geospatial Data",
            "isSensitive": true,
            "sensitivity": "critical",
            "patterns": [
                "(?i)\\b(live|real[-_]?time)[^\\s]{0,5}(gps|location|tracking)\\b",
                "\\b-?\\d{1,2}\\.\\d+,\\s*-?\\d{1,3}\\.\\d+\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Home Address",
