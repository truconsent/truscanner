            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\b(home|residential)[^\\s]{0,5}(address|location)\\b(?:\\s*[:=])"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
