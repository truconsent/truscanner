{
    "sources": [
        {
            "name": "IP Address",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "\\b(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\.){3}(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\b",
                "\\b(?:[A-F0-9]{1,4}:){7}[A-F0-9]{1,4}\\b",
                "(?i)\\b(?:(?<!sh)(?<!tr)(?<!sh-)(?<!tr-)(?<!sh_)(?<!tr_)ip|ipv4|ipv6)\\b(?![^\\s/(;)#|,=!>]*?(?:description|tooltip|metadata|Relationships|platform|standalone|clone|float|long|distance|translation|relative|calculate|update))",
                "(?i)(?:(?<!sh)(?<!tr)(?<!sh-)(?<!tr-)(?<!sh_)(?<!tr_)ip|ipv4|ipv6)[^\\s/(;)#|,=!>]{0,10}address|machine[^\\s/(;)#|,=!>]{0,10}ip"
            ],
            "tags": {}
        },
        {
            "name": "Device ID",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)\\bdevice[^\\w]{1,5}(id|identifier|uuid)\\b(?:\\s*[:=])",
                "(?i)\\bdevice[^\\s/(;)#|,=!>]{0,5}(?:id|info|details|token)\\b(?![^\\s/(;)#|,=!>]*?(?:width|height|pixel|ratio|orientation))",
                "imei"
            ],
            "tags": {}
        },
        {
            "name": "MAC Address",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "\\b(?:[0-9A-Fa-f]{2}[:-]){5}(?:[0-9A-Fa-f]{2})\\b",
                "(?i)(mac|ieee)[^\\s/(;)#|,=!>]{0,2}(?:address|ip)|ip[^\\s/(;)#|,=!>]{0,2}mac"
            ],
            "tags": {}
        },
        {
            "name": "Browser Fingerprint",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\bbrowser[^\\s]{0,5}(fingerprint|profile|hash)\\b"
            ],
            "tags": {}
        },
        {
            "name": "Cookies",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)\\b(cookie|session[_-]?cookie|tracking[_-]?cookie)\\b",
                "(?i).*(cookie([s]?$))"
            ],
            "tags": {}
        },
        {
            "name": "Tracking Technologies",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\b(tracking|analytics|pixel|beacon)\\b",
                "(?i).*(online[^\\s/(;)#|,=!>]{0,10}identifier|online[^\\s/(;)#|,=!>]{0,10}identity|(tracking|ad|advertising)[^\\s/(;)#|,=!>]{0,10}pixel)"
            ],
            "tags": {}
        },
        {
            "name": "Google ID",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\bgoogle[^\\s]{0,5}(id|account|user)\\b"
            ],
            "tags": {}
        },
        {
            "name": "Apple ID",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\bapple[^\\s]{0,5}(id|account)\\b"
            ],
            "tags": {}
        },
        {
            "name": "Facebook ID",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\b(facebook|fb)[^\\s]{0,5}(id|account|user)\\b"
            ],
            "tags": {}
        },
        {
            "name": "Session IDs",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "critical",
            "patterns": [
                "(?i)\\bsession[^\\w]{1,5}(id|token|key)\\b(?:\\s*[:=])"
            ],
            "tags": {}
        },
        {
            "name": "API Keys",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "critical",
            "patterns": [
                "(?i)\\bapi[^\\w]{1,5}(key|secret|token)\\b(?:\\s*[:=])",
                "(?i)\\bapi_key\\b"
            ],
            "tags": {}
        },
        {
            "name": "OAuth Tokens",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "critical",
            "patterns": [
                "(?i)\\boauth[^\\w]{1,5}(token|access|refresh)\\b(?:\\s*[:=])"
            ],
            "tags": {}
        },
        {
            "name": "Device Model",
            "category": "Digital & Online Identifiers",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)\\bdevice[^\\s]{0,5}(model|type)\\b"
            ],
            "tags": {}
        },
        {
            "name": "Operating System Version",
            "category": "Digital & Online Identifiers",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)\\b(os|operating)[^\\s]{0,5}(system|version)\\b"
            ],
            "tags": {}
        },
        {
            "name": "App Version",
            "category": "Digital & Online Identifiers",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)\\bapp[^\\s]{0,5}(version|build)\\b"
            ],
            "tags": {}
        },
        {
            "name": "User Agent String",
            "category": "Digital & Online Identifiers",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)\\buser-agent\\b(?!(?::\\s*(?:\\*|googlebot|bingbot|twitterbot|baiduspider|facebookexternalhit)))",
                "(?i)\\b(mozilla|applewebkit|chrome|safari|firefox)/[0-9.]+\\b"
            ],
            "tags": {}
        },
        {
            "name": "Advertising Identifiers",
            "category": "Digital & Online Identifiers",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)idfa|idfv|aaid|advertising[^\\s/(;)#|,=!>]{0,5}id|gps[^\\s/(;)#|,=!>]{0,5}adid"
            ],
            "tags": {}
        }
    ]
}
