{
    "sources": [
        {
            "name": "Bank Account Number",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "\\b\\d{9,18}\\b",
                "(?i).*((?<!question)bank[^\\s/(;)#|,=!>]*(?:account|details|detail|(swift|bic)-code|(swift|bic)_code)[^\\s/(;)#|,=!>]*(number|code)?|bank[^\\s/(;)#|,=!>]*account[^\\s/(;)#|,=!>]*details|(swift|bic)[-_]code|(swift|bic)code)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "IBAN",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "\\b[A-Z]{2}\\d{2}[A-Z0-9]{11,30}\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Routing Number",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "\\b\\d{9}\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Credit Card Number",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "\\b(?:4[0-9]{12}(?:[0-9]{3})?|5[1-5][0-9]{14}|3[47][0-9]{13}|6(?:011|5[0-9]{2})[0-9]{12})\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Debit Card Number",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "\\b\\d{16}\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "CVV",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(?:cvv|cvc|security[_-]?code)[^0-9]{0,5}\\b(\\d{3,4})\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Card Expiry Date",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "\\b(0[1-9]|1[0-2])\\/([0-9]{2}|[0-9]{4})\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "UPI ID",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\b[a-zA-Z0-9._-]{2,}@(apl|upi|ybl|okhdfcbank|okicici|paytm|okaxis|barodampay|axisbank|dbs|freecharge|hsbc|icici|idbi|indianbank|indus|kotak|payzapp|sib|waaxis|washdfc|waicici|wasbi)\\b",
                "(?i)(upi[^\\s]{0,5}id|vpa)[^\\w]{1,5}[a-zA-Z0-9._-]{2,}@[a-zA-Z]{2,}",
                "(?i).*(virtual[^\\s/(;)#|,=!>]{0,5}payment[^\\s/(;)#|,=!>]{0,5}address|virtual[^\\s/(;)#|,=!>]{0,5}payment|upi[-_<'.]{0,2}id)|(merchant|payee|payment|retailer|dealer|seller|vendor)[-_]{0,1}vpa|vpa[-_]{0,1}id"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Digital Wallet ID",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(wallet|paytm|phonepe|gpay|paypal)[^\\s]{0,5}(id|account)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Transaction History",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(transaction|txn)[^\\s]{0,5}(history|details|records)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Account Balance",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(account)[^\\s]{0,5}(balance|amount)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Credit Limit",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)(credit)[^\\s]{0,5}(limit)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Available Credit",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)(available)[^\\s]{0,5}(credit)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Loan Details",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(loan|borrowing)[^\\s]{0,5}(details|info|account)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "EMI Information",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(emi|installment)[^\\s]{0,5}(details|amount|schedule)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Investment Portfolio",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(investment|portfolio)[^\\s]{0,5}(details|holdings)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Mutual Fund Holdings",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(mutual)[^\\s]{0,5}(fund)[^\\s]{0,5}(holdings|investment)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Insurance Policies",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(insurance)[^\\s]{0,5}(policy|details|number)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Tax Returns",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(tax)[^\\s]{0,5}(return|itr|filing)",
                "(?i)(tax[^\\s/(;)#|,=!>]{0,5}(?:information|group|rate|info|percent|statement|detail|amount|calculate|type|slip|number)|taxable[^\\s/(;)#|,=!>]{0,10}value|(?:standard|income|without)[^\\s/(;)#|,=!>]{0,10}tax|(form|name|box|item)1099)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Income Statements",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(income)[^\\s]{0,5}(statement|salary|earnings)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Expense Categories",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)(expense)[^\\s]{0,5}(category|breakdown|type)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Recurring Payments",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)(recurring|auto)[^\\s]{0,5}(payment|debit)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Standing Instructions",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)(standing)[^\\s]{0,5}(instruction|mandate)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Card Issuer",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "high",
            "patterns": [
                "(?i).*((?:credit|debit)[^\\s/(;)#|,=!>]{0,10}card[^\\s/(;)#|,=!>]{0,10}issuer|card[^\\s/(;)#|,=!>]{0,10}issuer)"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Payment Nonce",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i)(.*)(payment[s]?)(nonce)(.*)"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Payment Mode",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i).*(payment[^\\s/(;)#|,=!>]{0,5}(?:mode|method|option|options|methodtype))"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Credit Score",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "high",
            "patterns": [
                "(?i).*((?:credit|cibil|crisil|fico|schufa)[^\\s/(;)#|,=!>]{0,10}score)"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Retirement Account Information (401k)",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "high",
            "patterns": [
                "(?i).*(401k|(?:retirement|pension)[^\\s/(;)#|,=!>]{0,10}(?:plan|account)).*"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Payroll Information",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "high",
            "patterns": [
                "(?i).*(payroll|pay[-_]roll).*"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Reimbursable Expenses",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i).*((?:travel|lodging|food|mileage|healthcare|business|medical|travel)[^\\s/(;)#|,=!>]{0,10}(?:reimbursement|reimbursements|expense))"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Company Allowances (HRA, etc)",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i).*((?:dearness|houserent|entertainment|overtime|compensatory|conveyance|leave[^\\s/(;)#|,=!>]{0,10}travel|medical|periodicals|books|withholding|personal|employment|child|basic[^\\s/(;)#|,=!>]{0,10}tax|travel|maternity|paternity)[^\\s/(;)#|,=!>]{0,10}allowance)"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Stock or Equity Grants",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "high",
            "patterns": [
                "(?i).*((?:user|employee|incentive|phantom|common|restricted)[^\\s/(;)#|,=!>]{0,10}(?:stocks|equity)|stocks[^\\s/(;)#|,=!>]{0,10}grant|employee[^\\s/(;)#|,=!>]{0,10}stock[^\\s/(;)#|,=!>]{0,10}purchase[^\\s/(;)#|,=!>]{0,10}plan|incentive[^\\s/(;)#|,=!>]{0,10}stock[^\\s/(;)#|,=!>]{0,10}option|employee[^\\s/(;)#|,=!>]{0,10}stock[^\\s/(;)#|,=!>]{0,10}option[^\\s/(;)#|,=!>]{0,10}scheme|restricted[^\\s/(;)#|,=!>]{0,10}stock[^\\s/(;)#|,=!>]{0,10}units|stock[^\\s/(;)#|,=!>]{0,10}appreciation[^\\s/(;)#|,=!>]{0,10}rights|phantom[^\\s/(;)#|,=!>]{0,10}stocks|non[^\\s/(;)#|,=!>]{0,10}qualified[^\\s/(;)#|,=!>]{0,10}stock[^\\s/(;)#|,=!>]{0,10}options|stock[^\\s/(;)#|,=!>]{0,10}option[^\\s/(;)#|,=!>]{0,10}grant|restricted[^\\s/(;)#|,=!>]{0,10}stock[^\\s/(;)#|,=!>]{0,10}awards)"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Benefits & Perks",
            "category": "Financial & Payment Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i).*(gym[^\\s/(;)#|,=!>]{0,10}membership|free[^\\s/(;)#|,=!>]{0,10}(?:lunch|snacks|breakfast|drinks|dinner)|employee[^\\s/(;)#|,=!>]{0,10}discount|employee[^\\s/(;)#|,=!>]{0,10}assistance[^\\s/(;)#|,=!>]{0,10}program|company[^\\s/(;)#|,=!>]{0,10}car|(?:retirement|pension)[^\\s/(;)#|,=!>]{0,10}plan|performance[^\\s/(;)#|,=!>]{0,10}bonus|(?:life|medical|dental|vision)[^\\s/(;)#|,=!>]{0,10}insurance|flexible[^\\s/(;)#|,=!>]{0,10}hours|company[^\\s/(;)#|,=!>]{0,10}shuttle|relocation[^\\s/(;)#|,=!>]{0,10}bonus|(?:paternity|maternity)[^\\s/(;)#|,=!>]{0,10}leave)"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Insurance Number / Policy Identifier",
            "category": "Financial & Payment Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i).*(insurance[^\\s/(;)#|,=!>]*(?:policy|plan|card|claim)|(?:insurance)[^\\s/(;)#|,=!>]*(?:card|policy|claim|plan|company)|card[^\\s/(;)#|,=!>]*insurance|insurance[^\\s/(;)#|,=!>]*policy[^\\s/(;)#|,=!>]*(?:nbr|number|id|no)|insurance[^\\s/(;)#|,=!>]*claim[^\\s/(;)#|,=!>]*(?:nbr|number|id|no)|ni[-_]?(?:nbr|no|number))"
            ],
            "tags": {
                "law": "GDPR"
            }
        }
    ]
}