            "name": "API Usage Logs",
            "category": "Technical & System Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i)\\bapi[^\\s]{0,5}(usage|log|activity|record)\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
