            "name": "Educational Qualifications",
            "category": "Educational & Academic Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\b(educational|academic|degree)[^\\s]{0,5}(qualification|credential|certificate)\\b"
