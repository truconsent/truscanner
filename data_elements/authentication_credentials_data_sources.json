{
    "sources": [
        {
            "name": "Username",
            "category": "Authentication & Credentials",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)\\b(user[^\\s/(;)#|,=!>]{0,5}id|account[^\\s/(;)#|,=!>]{0,5}id|member[^\\s/(;)#|,=!>]{0,5}id)\\b",
                "(?i)\\b(?<!id=['\"])(?<!name=['\"])(user[^\\s/(;)#|,=!>]{0,10}(?:name|id|handle)|account[^\\s/(;)#|,=!>]{0,10}(?:name|id)|member[^\\s/(;)#|,=!>]{0,10}id)\\b(?![^\\s/(;)#|,=!>]*?(?:Relationships|queryKey))"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Passwords",
            "category": "Authentication & Credentials",
            "isSensitive": true,
            "sensitivity": "critical",
            "patterns": [
                "(?i)\\b(pass[^\\s/(;)#|,=!>]{0,10}(?:word|phrase|code|key|secret)|pwd|credential[^\\s/(;)#|,=!>]{0,5}secret|account[^\\s/(;)#|,=!>]{0,5}secret)\\b",
                "(?i)\\b(?<!id=['\"])(?<!name=['\"])(?:pass|pwd)[^\\s/(;)#|,=!>]{0,10}(?:word|phrase|code|key|secret|credential)\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Security Questions",
            "category": "Authentication & Credentials",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)(security)[^\\s]{0,5}(question|challenge)"
