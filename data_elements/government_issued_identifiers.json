{
    "sources": [
        {
            "name": "Aadhaar Number (India)",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "\\b\\d{4}\\s?\\d{4}\\s?\\d{4}\\b",
                "(?i)\\b(?:aadhaar|aadhar)[^\\s/(;)#|,=!>]{0,10}(?:number|no|id)\\b",
                "(?i)\\baadhaar[^\\s/(;)#|,=!>]{0,5}number[^\\s/(;)#|,=!>]{0,5}india\\b",
                "(?i)\\baadhaar[-_\\s]{0,2}number[-_\\s]{0,2}india\\b"
            ]
        },
        {
            "name": "PAN (Permanent Account Number) (India)",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "\\b[A-Z]{5}\\d{4}[A-Z]{1}\\b",
                "(?i)\\b(?:pan|permanent[^\\s/(;)#|,=!>]{0,5}account[^\\s/(;)#|,=!>]{0,5}(?:number|no))\\b",
                "(?i)\\bpan[^\\s/(;)#|,=!>]{0,5}permanent[^\\s/(;)#|,=!>]{0,5}account[^\\s/(;)#|,=!>]{0,5}number[^\\s/(;)#|,=!>]{0,5}india\\b",
                "(?i)\\bpan[-_\\s]{0,2}permanent[-_\\s]{0,2}account[-_\\s]{0,2}number[-_\\s]{0,2}india\\b"
            ]
        },
        {
            "name": "SSN (Social Security Number) (US)",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "\\b\\d{3}-\\d{2}-\\d{4}\\b",
                "(?i)\\b(?:ssn|social[^\\s/(;)#|,=!>]{0,5}security[^\\s/(;)#|,=!>]{0,5}(?:number|no))\\b",
                "(?i)\\bssn[^\\s/(;)#|,=!>]{0,5}social[^\\s/(;)#|,=!>]{0,5}security[^\\s/(;)#|,=!>]{0,5}number\\b",
                "(?i)\\bssn[-_\\s]{0,2}social[-_\\s]{0,2}security[-_\\s]{0,2}number\\b"
            ]
        },
        {
            "name": "Passport Number",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "\\b[A-Z]{1,2}\\d{6,9}\\b",
                "(?i)\\b(?:passport)[^\\s/(;)#|,=!>]{0,10}(?:number|no|id)\\b",
                "(?i)\\bpassport[^\\s/(;)#|,=!>]{0,5}number\\b",
                "(?i)\\bpassport[-_\\s]{0,2}number\\b"
            ]
        },
        {
            "name": "Driver's License Number",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "(?i)\\bdriver[^\\s/(;)#|,=!>]{0,5}license[^\\s/(;)#|,=!>]{0,5}number\\b",
                "(?i)\\bdriver[-_\\s]{0,2}license[-_\\s]{0,2}number\\b"
            ]
        },
        {
            "name": "Voter ID Number",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "(?i)\\bvoter[^\\s/(;)#|,=!>]{0,5}number\\b",
                "(?i)\\bvoter[-_\\s]{0,2}number\\b"
            ]
        },
        {
            "name": "Tax Identification Number",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "(?i)\\btax[^\\s/(;)#|,=!>]{0,5}identification[^\\s/(;)#|,=!>]{0,5}number\\b",
                "(?i)\\btax[-_\\s]{0,2}identification[-_\\s]{0,2}number\\b"
            ]
        },
        {
            "name": "GST Number",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "(?i)\\bgst[^\\s/(;)#|,=!>]{0,5}number\\b",
                "(?i)\\bgst[-_\\s]{0,2}number\\b"
            ]
        },
        {
            "name": "Business Registration Number",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "(?i)\\bbusiness[^\\s/(;)#|,=!>]{0,5}registration[^\\s/(;)#|,=!>]{0,5}number\\b",
                "(?i)\\bbusiness[-_\\s]{0,2}registration[-_\\s]{0,2}number\\b"
            ]
        },
        {
            "name": "Professional License Numbers",
            "category": "Government-Issued Identifiers",
            "patterns": [
                "(?i)\\bprofessional[^\\s/(;)#|,=!>]{0,5}license[^\\s/(;)#|,=!>]{0,5}numbers\\b",
                "(?i)\\bprofessional[-_\\s]{0,2}license[-_\\s]{0,2}numbers\\b"
            ]
        }
    ]
}