{
    "sources": [
        {
            "name": "Date of Birth",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)(dob|(.*(date[^\\s/(;)#|,=!>]{0,5}of[^\\s/(;)#|,=!>]{0,5}birth|birth[^\\s/(;)#|,=!>]{0,5}(?:day|date|month|year)|birth[-_]{0,2}dt)))"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "First Name",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i).*((?:first|given)[^\\s/(;)#|,=!>]{0,5}|fore[-_]?|full[-_]?)name|f[_]?name"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Gender",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)((.*gender)|is[_-]{0,1}male|is[_-]{0,1}female|sex)"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Last Name",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i).*((?:last|sur(?!geon))[^\\s/(;)#|,=!>]{0,5}name)|l[_]?name"
