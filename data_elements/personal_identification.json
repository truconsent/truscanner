{
    "sources": [
        {
            "name": "Date of Birth",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)(dob|(.*(date[^\\s/(;)#|,=!>]{0,5}of[^\\s/(;)#|,=!>]{0,5}birth|birth[^\\s/(;)#|,=!>]{0,5}(?:day|date|month|year)|birth[-_]{0,2}dt)))"
            ],
            "tags": {}
        },
        {
            "name": "First Name",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i).*((?:first|given)[^\\s/(;)#|,=!>]{0,5}|fore[-_]?|full[-_]?)name|f[_]?name"
            ],
            "tags": {}
        },
        {
            "name": "Gender",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)((.*gender)|is[_-]{0,1}male|is[_-]{0,1}female|sex)"
            ],
            "tags": {}
        },
        {
            "name": "Last Name",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i).*((?:last|sur(?!geon))[^\\s/(;)#|,=!>]{0,5}name)|l[_]?name"
            ],
            "tags": {}
        },
        {
            "name": "Age",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)^(user|person|customer|under|years|yrs|human)?(_)?(of|in)?(_)?(age)$",
                "(?i)^(age)(_)?(of|in)?(_)?(human|years|yrs|user|person|customer)?$"
            ],
            "tags": {}
        },
        {
            "name": "Employee Code",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)(?:.*employee|emp)[^\\s/(;)#|,=!>]{0,10}(?:no|number|code|id|nbr|num)"
            ],
            "tags": {}
        },
        {
            "name": "Photograph",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i).*(?:profile|employee|user)[^\\s/(;)#|,=!>]{0,5}(?:picture|image|pic|photo)"
            ],
            "tags": {}
        },
        {
            "name": "Middle Name",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)\\b(middle[^\\s/(;)#|,=!>]{0,5}name|m[_]?name)\\b"
            ],
            "tags": {}
        },
        {
            "name": "Alias",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)(?<!\")\\b(alias|nick[^\\s/(;)#|,=!>]{0,5}name|aka|also[^\\s/(;)#|,=!>]{0,5}known[^\\s/(;)#|,=!>]{0,5}as)\\b"
            ],
            "tags": {}
        },
        {
            "name": "Nationality",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i).*(nationality|citizen[^\\s/(;)#|,=!>]{0,5}ship|country[^\\s/(;)#|,=!>]{0,5}of[^\\s/(;)#|,=!>]{0,5}origin)",
                "(?i).*(nationality|nationalities)"
            ],
            "tags": {}
        },
        {
            "name": "Marital Status",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)(?<!\\.)\\b(marital[^\\s/(;)#|,=!>]{0,5}status|married|single|divorced|widowed|relationship[^\\s/(;)#|,=!>]{0,5}status)\\b(?!\\()",
                "(?i).*(marital|married|relationship[^\\s/(;)#|,=!>]{0,2}status).*"
            ],
            "tags": {}
        },
        {
            "name": "Place of Birth",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i).*(place[^\\s/(;)#|,=!>]{0,5}of[^\\s/(;)#|,=!>]{0,5}birth|birth[^\\s/(;)#|,=!>]{0,5}place|pob)"
            ],
            "tags": {}
        },
        {
            "name": "Mother's Maiden Name",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i).*(mother[^\\s/(;)#|,=!>]{0,5}maiden[^\\s/(;)#|,=!>]{0,5}name|mother[^\\s/(;)#|,=!>]{0,5}name|maternal[^\\s/(;)#|,=!>]{0,5}name)",
                "(?i).*(?:guardian|mother|father)[^\\s/(;)#|,=!>]{0,8}name|name[^\\s/(;)#|,=!>]{0,8}(?:guardian|mother|father).*"
            ],
            "tags": {}
        },
        {
            "name": "Father's Name",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i).*(father[^\\s/(;)#|,=!>]{0,5}name|paternal[^\\s/(;)#|,=!>]{0,5}name)"
            ],
            "tags": {}
        },
        {
            "name": "Spouse Name",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i).*(spouse[^\\s/(;)#|,=!>]{0,5}name|partner[^\\s/(;)#|,=!>]{0,5}name|husband[^\\s/(;)#|,=!>]{0,5}name|wife[^\\s/(;)#|,=!>]{0,5}name)",
                "(?i).*(?:fiance|spouse|husband|wife)[^\\s/(;)#|,=!>]{0,8}name.*"
            ],
            "tags": {}
        },
        {
            "name": "Emergency Contact Details",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i).*(emergency[^\\s/(;)#|,=!>]{0,5}contact|emergency[^\\s/(;)#|,=!>]{0,5}(?:phone|number|email|person))"
            ],
            "tags": {}
        },
        {
            "name": "Physical Appearance Description",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i)(?<!height:)(?<!weight:)(?<!Build\\sa\\s)(?<!//\\s)\\b(physical[^\\s/(;)#|,=!>]{0,5}(?:appearance|description)|height|weight|eye[^\\s/(;)#|,=!>]{0,5}color|hair[^\\s/(;)#|,=!>]{0,5}color|complexion|build)\\b(?!:\\s*?[0-9])",
                "(?i)body[^\\s/(;)#|,=!>]{0,5}mass[^\\s/(;)#|,=!>]{0,5}index|body[^\\s/(;)#|,=!>]{0,5}weight|weight[^\\s/(;)#|,=!>]{0,5}(?:kg|kgs|pound|pounds)|(?:body|person|patient|baby|student|user|girl|boy|male|female)[^\\s/(;)#|,=!>]{0,5}weight"
            ],
            "tags": {}
        },
        {
            "name": "Racial or Ethnic Origin",
            "category": "Personal Identification",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i).*((?:racial|ethnic)[^\\s/(;)#|,=!>]{0,5}(?:origin|group)|ethnicity)"
            ],
            "tags": {}
        },
        {
            "name": "Religion or Religious Beliefs",
            "category": "Personal Identification",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i).*(religion|religious[^\\s/(;)#|,=!>]{0,5}(group|community|practices|belief|beleifs))"
            ],
            "tags": {}
        },
        {
            "name": "Sexual Orientation",
            "category": "Personal Identification",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i).*(sexual[^\\s/(;)#|,=!>]{0,5}(orientation|nature|preference|inclination))"
            ],
            "tags": {}
        },
        {
            "name": "Signature",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "high",
            "patterns": [
                "(?i)(?:person|customer|user|buyer|seller|publisher|CEO|CFO)[^\\s/(;)#|,=!>]{0,5}signature"
            ],
            "tags": {}
        },
        {
            "name": "Dependent Name",
            "category": "Personal Identification",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i).*(?:dependant|dependent|daughter|children|wife|husband|fiance|grand[^\\s/(;)#|,=!>]{0,8}mother|grand[^\\s/(;)#|,=!>]{0,8}father)[^\\s/(;)#|,=!>]{0,8}name.*"
            ],
            "tags": {}
        }
    ]
}
