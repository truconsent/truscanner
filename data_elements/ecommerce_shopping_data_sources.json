{
    "sources": [
        {
            "name": "Purchase History",
            "category": "eCommerce & Shopping Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\b(purchase|order)[^\\s]{0,5}(history|record|log)\\b",
                "(?i)purchase[^\\s/(;)#|,=!>]{0,5}(?:history|(?<!site)(?<!val)id)|past[^\\s/(;)#|,=!>]{0,5}(?:purchases|purchase)|previous[^\\s/(;)#|,=!>]{0,5}purchase"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Order Details",
            "category": "eCommerce & Shopping Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
