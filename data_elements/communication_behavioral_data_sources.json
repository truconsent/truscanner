        {
            "name": "Error Logs",
            "category": "Communication & Behavioral Data",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)\\b(?<!console\\.)(?<!throw\\snew\\s)(?<!const\\s)(?<!const\\s\\{)(?<!let\\s)(?<!let\\s\\{)(?<!var\\s)(?<!var\\s\\{)(error|exception|stacktrace)\\b(?:\\s*[:=])",
                "(?i)\\b(error|exception|stacktrace)[^\\s]{0,5}(details|message)\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Feedback",
            "category": "Communication & Behavioral Data",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)\\b(feedback|suggestion)\\b",
                "(?i)\\bcomment\\b(?:\\s*[:=])"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Reviews",
            "category": "Communication & Behavioral Data",
            "isSensitive": false,
            "sensitivity": "low",
            "patterns": [
                "(?i)\\b(review|rating|testimonial)\\b(?:\\s*[:=])"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Survey Responses",
            "category": "Communication & Behavioral Data",
            "isSensitive": true,
            "sensitivity": "medium",
            "patterns": [
                "(?i)\\b(survey|questionnaire)[^\\s]{0,5}(response|answer|result)\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
            "name": "Ratings",
            "category": "Communication & Behavioral Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i)((?:user|product|movie|show|drama|coach|buyer|seller|doctor|hospital|college|hotel|agent|author|customer|artist|photo|policyholder|insurer|claimant|average|total)[^\\s/(;)#|,=!>]{0,5}){0,1}(?:rating|ratings)"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Detailed Reviews",
            "category": "Communication & Behavioral Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i)\\b(((?:user|movie|drama|coach|buyer|seller|doctor|hospital|college|hotel|agent|policyholder|insurer|claimant)[^\\s/(;)#|,=!>]{0,4}){0,1}(?:review|reviews))\\b"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Communication Content",
            "category": "Communication & Behavioral Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i)(email[^\\s/(;)#|,=!>]{0,5}(?:content|message|msg|text))"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Support Records",
            "category": "Communication & Behavioral Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i)support[^\\s/(;)#|,=!>]{0,10}ticket|customer[^\\s/(;)#|,=!>]{0,12}(?:complaint|support)"
            ],
            "tags": {
                "law": "GDPR"
            }
        },
        {
            "name": "Customer Sentiment (NPS/CSAT)",
            "category": "Communication & Behavioral Data",
            "isSensitive": false,
            "sensitivity": "medium",
            "patterns": [
                "(?i).*(((?:nps|csat|net[^\\s/(;)#|,=!>]{0,15}promoter)([^\\s/(;)#|,=!>]{0,15}(?:score|feedback)){0,1}|customer[^\\s/(;)#|,=!>]{0,15}satisfaction)|(product[^\\s/(;)#|,=!>]{0,10}feedback|(feedback|survey)[^\\s/(;)#|,=!>]{0,5}(response|result))).*|feedback"
            ],
            "tags": {
                "law": "GDPR"
            }
        }
    ]
}