{
    "sources": [
        {
            "name": "Call Records",
            "category": "Communication & Behavioral Data",
            "isSensitive": true,
            "sensitivity": "high",
            "patterns": [
                "(?i)\\bcall[^\\s]{0,5}(record|log|history|detail)\\b",
                "(?i)\\b(cdr|call[-_]?detail[-_]?record)\\b"
            ],
            "tags": {
                "law": "DPDPA"
            }
        },
        {
